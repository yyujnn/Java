package 이중for문;

public class ex03별찍기 {

	public static void main(String[] args) {

		
		// 역삼각형
		for (int j = 1; j <= 5; j++) {

			for (int i = 5; i >= j; i--) {

				System.out.print("*");
			}
			System.out.println("");
		}

		

		
		System.out.println("    ");
		System.out.println("*");
		System.out.println();
		
		System.out.println("   ");
		System.out.println("**");
		System.out.println();
		
		
		
		
		
		
		
	}

}
